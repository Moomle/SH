package ffof.express.model;

import java.util.Date;


/**
 * @author Abner
 * 
 * 	int id, identify number, db type 
	int FK_ord_num, order number, db type bigint
	int cur_id, cursour id, db type bigint, 
	String org_plc, origin place, db type varchar(40)
	String dst_plc, destination place, db type varchar(40)
	Date crt_time, create time, db type timestamp
	int stat, current stat, db type tinyint, default value is 0, value 1 means order is delivered.
 *
 */
public class Order {
	
	private int id;
	private String ord_num;
	private String cur_phone;
	private String org_plc;
	private String org_name;
	private String org_phone;
	private String dst_plc;
	private String dst_name;
	private String dst_phone;
	private String location;
	private Date crt_time;
	private String stat;
	
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public String getOrd_num() {
		return ord_num;
	}
	public void setOrd_num(String ord_num) {
		this.ord_num = ord_num;
	}
	
	public String getCur_phone() {
		return cur_phone;
	}
	public void setCur_phone(String cur_phone) {
		this.cur_phone = cur_phone;
	}
	public String getOrg_plc() {
		return org_plc;
	}
	public void setOrg_plc(String org_plc) {
		this.org_plc = org_plc;
	}
	public String getOrg_name() {
		return org_name;
	}
	public void setOrg_name(String org_name) {
		this.org_name = org_name;
	}
	public String getOrg_phone() {
		return org_phone;
	}
	public void setOrg_phone(String org_phone) {
		this.org_phone = org_phone;
	}
	public String getDst_plc() {
		return dst_plc;
	}
	public void setDst_plc(String dst_plc) {
		this.dst_plc = dst_plc;
	}
	public String getDst_name() {
		return dst_name;
	}
	public void setDst_name(String dst_name) {
		this.dst_name = dst_name;
	}
	public String getDst_phone() {
		return dst_phone;
	}
	public void setDst_phone(String dst_phone) {
		this.dst_phone = dst_phone;
	}
	public Date getCrt_time() {
		return crt_time;
	}
	public void setCrt_time(Date crt_time) {
		this.crt_time = crt_time;
	}
	public String getStat() {
		return stat;
	}
	public void setStat(String stat) {
		this.stat = stat;
	}
	
	
	
}
