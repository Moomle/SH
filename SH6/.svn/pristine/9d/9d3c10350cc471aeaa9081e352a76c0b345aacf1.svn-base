.catenav-mod dl{position:relative;}
.catenav-mod dt{position:absolute;top:0;left:0;padding:0 0 0 20px;width:80px;font-size:14px;line-height:30px;}
.catenav-mod dd{padding:0 0 0 120px;}
.catenav-mod dd ul{overflow:hidden;zoom:1;}
.catenav-mod dd li{float:left;padding:0 0 0 4px;width:100px;height:30px;font-size:14px;line-height:30px;}
.catenav-mod dd li a,.catenav-mod .market-list li a,.catenav-mod .list li a{color:#333;}
.catenav-mod .list,.catenav-mod .market-list{overflow:hidden;zoom:1;}
.catenav-mod .list li{float:left;margin:0 20px;height:30px;font-size:14px;line-height:30px;}
